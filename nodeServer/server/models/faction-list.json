{
  "name": "factionList",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "headerImage": {
      "type": "string"
    },
    "updateTime": {
      "type": "date"
    },
    "sectionArray": {
      "type": [
        "string"
      ],
      "required": true,
      "default": [
        "undefined"
      ]
    },
    "author": {
      "type": "string",
      "comments": "小说作者"
    },
    "des": {
      "type": "string",
      "comments": "小说的简要描述"
    },
    "factionName": {
      "type": "string",
      "comments": "小说名"
    }
  },
  "validations": [],
  "relations": {
    "sectionArray": {
      "type": "hasMany",
      "model": "factionContent",
      "foreignKey": "id",
      "through": "factionList"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
